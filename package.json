{
  "name": "nikhil-pise-portfolio",
  "version": "2.0.0",
  "description": "Personal portfolio website for Nikhil Pise - Machine Learning & Data Science student",
  "main": "index.html",
  "scripts": {
    "dev": "python3 -m http.server 8000",
    "lint:js": "eslint js/**/*.js",
    "lint:css": "stylelint css/**/*.css",
    "format": "prettier --write \"**/*.{html,css,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{html,css,js,json,md}\"",
    "validate:html": "html-validate \"*.html\"",
    "test": "echo \"No tests configured yet\" && exit 0",
    "build": "npm run format && npm run lint:js && npm run lint:css && npm run validate:html"
  },
  "keywords": [
    "portfolio",
    "machine-learning",
    "data-science",
    "AI",
    "deep-learning",
    "NLP"
  ],
  "author": "Nikhil Pise <nikhilpise2006@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/N1KH1LT0X1N/Nikhil-Portfolio.git"
  },
  "homepage": "https://github.com/N1KH1LT0X1N/Nikhil-Portfolio#readme",
  "devDependencies": {
    "eslint": "^8.50.0",
    "html-validate": "^8.5.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
